Module

#use "Libs/SCI/Sys"


Global Void CMBT_DEBUG_DIAMONDS()
	for i,v in pairs(Lua.Scyndi.Class.Diamonds.Slots)
		printf("Slot #%05d> X=%d; ToX=%d; Type=%s; ",i,v.X,v.ToX,v.Type)
		switch(v.Type)
			case "PLAYER" "FOE"
				printf("FighterTag = %s; ",v.FighterTag)
			case "JOKER"
				printf("Joker; ")
			case "BACK"
				printf("Inactive; ")
			default
		end
		printf("\n")
	end
End


Init
	Sys.ConsoleLocal("DIAMONDS","CMBT_DEBUG_DIAMONDS")	
End